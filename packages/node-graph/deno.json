{
    "name": "@collidor/node-graph",
    "version": "0.0.1",
    "license": "MIT",
    "exports": {
        ".": "./src/main.ts"
    },
    "publish": {
        "include": [
            "./src/**/*",
            "README.md",
            "deno.json",
            "deno.lock",
            "LICENSE"
        ],
        "exclude": [
            "dist"
        ]
    },
    "tasks": {
        "dev": "deno run --watch ./src/main.ts"
    },
    "imports": {
        "@std/assert": "jsr:@std/assert@1",
        "@std/testing": "jsr:@std/testing@1"
    },
    "lint": {
        "rules": {
            "exclude": [
                "no-explicit-any",
                "no-window",
                "no-window-prefix"
            ]
        }
    },
    "compilerOptions": {
        "lib": [
            "ESNext",
            "deno.window",
            "dom"
        ],
        "noUncheckedIndexedAccess": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "strict": true
    }
}