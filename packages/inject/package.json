{
    "name": "@collidor/injector",
    "version": "2.4.1",
    "description": "A tiny dependency injection container with type inference",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/collidor/collidor.git"
    },
    "type": "module",
    "main": "dist/main.cjs",
    "module": "dist/main.js",
    "types": "dist/main.d.ts",
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "test": "deno test",
        "lint": "deno lint",
        "test:coverage": "deno test --coverage && deno coverage --lcov --output=./coverage/lcov.info && deno coverage --html",
        "publish:npm": "NPM_CONFIG_PROVENANCE=true pnpm publish --access public",
        "publish:jsr": "npx jsr publish",
        "publish-package": "npm ci && npm run publish:npm && npm run publish:jsr"
    },
    "keywords": [
        "DI",
        "dependency injection",
        "container",
        "injector",
        "injection",
        "dependency",
        "injection container",
        "dependency injection container"
    ],
    "author": "Alykam Burdzaki <alykam@gmail.com> (https://alykam.com)",
    "license": "MIT",
    "devDependencies": {
        "@swc/core": "^1.10.14",
        "tsup": "^8.3.6",
        "typescript": "^5.7.3"
    }
}